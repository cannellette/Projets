--explosion

function create_explosion(x,y)
	add(explosion,{
		x=x,
		y=y,
		timer = 0
		})
		sfx(0)
end

function update_explosion()
	for boom in all(explosion) do
		boom.timer +=1
		if boom.timer == 13 then
			del(explosion,boom)
			end
		end
	end
	
function draw_explosion()
	for boom in all(explosion) do
		circ(boom.x,boom.y,
			boom.timer/3,7+boom.timer%3)
	end
end	

