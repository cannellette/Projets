function update_start()
	if (bgm==false) then
		music(2)
		bgm=true
	end
	if (btnp(â)) state=1
end

function draw_start()
	cls()
	map()
	
	palt(0,false)
	palt(1,true)
	
	//medic
	spr(3,56,96,2,2)
	
	//allies
	i=0
	for a in all(allies) do
		w=80
		if (a.w==1) w=88
		spr(a.sp,20+i*24,w,a.w,a.h)
		i+=1
	end
	
	spr(128,25,42,10,3)

	print("press â to play",30,60,7)

	//reload
	spr(30,10,113)
	print("ğŸ…¾ï¸:reload",20,114,8)
	
	//heal
	print("â:heal",76,102,8)
	
	//powerup
	spr(30,105,120,1,1,true)
	print("ğŸ…¾ï¸:powerup",64,121,8)
	draw_powerup()
	
	palt()
end


---------game screen------
function update_game()
	music(-1)
	bgm=false
	move()
	
	--shoot
	if (btnp(â) and start) 
	then
	 shoot_m()
	else start=true
	end
	

	update_powerup()
	

	update_bands()
	update_police()
	update_explosion()
	update_allies()
	
	--police
	if #police==0 then
		spawn_police(ceil(rnd(4)))
	end
	
	--reload bandages
	reload_bandages()
	
	--collision allies/bands
	collision_bands()
	
	--collision allies/police
	collision_police()
	
	--medic shoot animation
	animate_shoot()
	
	--chronometer
	update_gametime()
	
	--total anihilation button
	ko_btn()
	
end

function draw_game()
	cls()
	map()
	
	palt(0,false)
	palt(1,true)
	
	--allies
	draw_allies()
	
	--bandages
	draw_bands()
	
	--medic
	draw_player()
	
	--police bullets
	for p in all (police) do
	 spr((p.sprite),p.x,p.y)
	end
	
	--explosions
	draw_explosion()
	
	--powerup
	if (powerup.time==0) draw_powerup()
	
	--powerup countdown
	draw_pu_count()
	
		--bandages stock
	spr(16,1,120)	
	
	print(nb_bands,10,122,0)
	
	draw_gametime()

	pal()
end

