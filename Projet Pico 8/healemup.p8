function _init()
	state=0
	bgm=false
	start=false

	create_player()
	create_allies()
	create_powerup()
	
	bands={}
	police={}
	explosion={}
	
	pvmax=100
	nb_bands=20
	gametime=5400

	go_count=-1
	pu_count=-1
	
	bands_speed=1
end

function _update60()
	if (state==0) update_start()
	if (state==1) update_game()
	if (state==2) update_gameover()
	if (state==3) update_win()
end

function _draw()
	if (state==0) draw_start()
	if (state==1) draw_game()
	if (state==2) draw_gameover()
	if (state==3) draw_win()
end

