--police
function spawn_police(amount)

 gap=(128-8*amount)/(amount+1)
 
 for i=1,amount do
	 add (police, {
	 sprite=(rnd{11,26,27}),
	 x=gap*i +8*(i-1),
	 y=rnd(20),
	 speed=0.5
	 })
 end
end

function update_police()
 for pol in all(police) do
 	pol.y +=pol.speed
 	if pol.y > 90 then
 		del(police, pol)
 	end
 end
end	

function collision_police()
	for pol in all (police) do
		for a in all(allies) do
			if(collision(
						a,
						pol,
						a.w*8,
						a.h*8/2,
						8)
						and a.pv>0) 
			then
				create_explosion(pol.x,pol.y)
				del (police,pol)
				if (a.pv>0) a.pv-=10
				if (a.pv<0) a.pv=0
			end
		end
	end	
end
