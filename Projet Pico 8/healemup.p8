function update_gametime()
	if (gametime==0) then
		if (allies[1].pv>0 and
						allies[2].pv>0 and
						allies[3].pv>0 and
						allies[4].pv>0)
		then
			state=3
		else
			state=2
		end
	else
		gametime-=1
	end
end

function draw_gametime()
	local t=ceil(gametime/60)
	print(t,114,2,8)
end
