--tools

function collision(a,b,aw,ah,bs)
	if a.x > b.x +bs
	or a.y > b.y +bs
	or a.x +aw < b.x
	or a.y +ah < b.y then
		return false
	else
		return true
	end
end

function ko_btn()
	if (btn(3)) then
		for a in all(allies) do
			a.pv=0
		end
	end
end
