--allies

function create_allies()
	allies={
		--elf
		{x=38,
			y=32,
			sp=36,
			sp_down=44,
			w=2,
			h=2,
			pv=100,
			speed=0.5,
			flip=false,
			down=false,
			count=-1},
		--old
		{x=86,
			y=52,
			sp=34,
			sp_down=46,
			w=2,
			h=2,
			pv=100,
			speed=0.3,
			flip=false,
			down=false,
			count=-1},
		--bob
		{x=17,
			y=62,
			sp=38,
			sp_down=42,
			w=2,
			h=2,
			pv=100,
			speed=0.5,
			flip=false,
			up=true,
			count=0},
		--kid
		{x=68,
			y=88,
			sp=23,
			sp_down=31,
			w=1,
			h=1,
			pv=100,
			speed=0.7,
			flip=false,
			count=-1}
	}
end

function draw_allies()
	for a in all(allies) do
		local sprite=a.sp
		if (a.pv==0) sprite=a.sp_down
		spr(
			sprite,
			a.x,
			a.y,
			a.w,
			a.h,
			a.flip
		)
			
		w=a.x+4
		if (a.w==1)w=a.x
		
		if (a.pv==pvmax) spr(7,w,a.y-9)
		if (a.pv<100 and a.pv>=60) spr(8,w,a.y-9)
		if (a.pv<60 and a.pv>=30) spr(9,w,a.y-9)
		if (a.pv<30 and a.pv>0) spr(10,w,a.y-9)
	end
end

function update_allies()
	if (allies[1].pv>0) update_elf()
	if (allies[2].pv>0) update_old()
	if (allies[3].pv>0) update_bob()
	if (allies[4].pv>0) update_kid()

	if (allies[1].pv==0 and
					allies[2].pv==0 and
					allies[3].pv==0 and
					allies[4].pv==0)
	then
		if (go_count==-1) go_count=30
		if (go_count==0) then
			state=2
		else
			go_count-=1
		end
	end
					
end

function update_elf()
	local a = allies[1]
	
	//change sprite direction
	if (a.x<=33 and a.flip==true) then
	 a.down=true
	end
	if (a.x>=96) then
		a.flip=true
		if (a.count==-1) a.count=120
	end
	if (a.y>=70) then
		a.flip=false
		if (a.count==-1) a.count=180
	end
	if (a.y<=32 and a.flip==false) then
	 a.down=false
	end
	
	//move sprite
	if (a.count>0) then
		a.count-=1
	else
		a.count=-1
		if (a.down) then
			if (a.flip) a.y+=a.speed
			if (a.flip==false) a.y-=a.speed
		else
			if (a.flip) a.x-=a.speed
			if (a.flip==false) a.x+=a.speed
		end
	end

end

function update_old()
	local a = allies[2]
	
	//change sprite direction
	if (a.x<=54) then
	 a.flip=true
	 if (a.count==-1) a.count=180
	end
	if (a.x>=74 and a.flip==true) then
	 a.down=true
	end
	if (a.y>=70) then
	 a.flip=false
	 if (a.count==-1) a.count=120
	end
	if (a.y<=52 and a.flip==false) then
	 a.down=false
	end
	
		
	//move sprite
	if (a.count>0) then
		a.count-=1
	else
		a.count=-1
		if (a.down) then
			if (a.flip) a.y+=a.speed
			if (a.flip==false) a.y-=a.speed
		else
			if (a.flip==false) a.x-=a.speed
			if (a.flip==true) a.x+=a.speed
		end	
	end

end

function update_bob()
	local a = allies[3]
	
	//change direction
	if (a.y<=32) a.up=false
	if (a.y>=70) a.up=true
	
	//move sprite
	if (a.up==true) then
	 a.y-=a.speed
	end
	if (a.up==false) then
	 a.y+=a.speed
	end
	
	//animate sprite
	a.count+=1
	if (a.count==30) then
	 a.flip=not a.flip
	 a.count=0
	end
end

function update_kid()
	local a = allies[4]
	
	//change sprite direction
	if (a.x<=24) a.flip=false
	if (a.x>=96) a.flip=true
	
	//move sprite
	if (a.flip==true) a.x-=a.speed
	if (a.flip==false) a.x+=a.speed
end

