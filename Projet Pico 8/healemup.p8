--player

function create_player()
	med={
		x=56,
		y=104,
		spr_walk=3,
		spr_shoot=5,
		speed=1,
		flip=false,
		shoot=false,
		shoot_count=0
	}
end

function draw_player()
	if (med.shoot) then
		spr(5,med.x,med.y,2,2,med.flip)
	else spr(3,med.x,med.y,2,2,med.flip)
	end
	
	if (pu_count>=0) then
		spr(15,med.x,med.y-8)
		t=ceil(pu_count/60)
		print(t,med.x+10,med.y-7,0)
	end
end

function move()
	if (btn(â¡ï¸) and med.x<112) then
		med.x+=med.speed
		med.flip=true
	end
	if (btn(â¬…ï¸) and med.x>0) then
		med.x-=med.speed
		med.flip=false
	end
end

function animate_shoot()
	if (med.shoot_count>0) then
	 med.shoot_count-=1
	else med.shoot=false
	end
end
