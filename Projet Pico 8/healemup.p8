function create_powerup()
	powerup={
		time=180,
		sprite=138,
		w=2,
		h=1
	}
end

function draw_powerup()
	spr(powerup.sprite,112,119,powerup.w,powerup.h)
end

function update_powerup()
	--powerup countdonw to appear
	if (powerup.time>0) then
	 powerup.time-=1
	end
	
	--start powerup
	if (btnp(ğŸ…¾ï¸) and
					med.x>=104 and
					powerup.time==0 and
					pu_count==-1) 
	then
		pu_count=600
		powerup.time=1500
	end
	
	if(pu_count>=0) then
		bands_speed=2
		med.speed=2
		pu_count-=1
	else
		bands_speed=1
		med.speed=1
	end
end

function draw_pu_count()
	if (powerup.time>0) then
		t=ceil(powerup.time/60)
		print(t,116,120,0)
	end
end
