--bandages
function shoot_m()
if nb_bands>0 then
		local new_band={
			x=med.x,
			y=med.y,
			speed=bands_speed
		}
		add(bands, new_band)
		
		//start shoot animation
		med.shoot=true
		med.shoot_count=20
		
		nb_bands-=1
	end
end

function draw_bands()
	for b in all(bands) do
		spr(1,b.x,b.y, 2,2)
	end
end

function update_bands()
 for b in all(bands) do
	 b.y-=b.speed
	 if (b.y==16) del(bands,b)
 end
end

function collision_bands()
	for b in all(bands) do
		for a in all(allies) do
			if (collision(
					a,
					b,
					a.w*8,
					a.h*8/2,
					16
				)) then
				sfx(1)
				del(bands,b)
				if (a.pv<pvmax) a.pv+=5	
			end
		end
	end
end

function reload_bandages()
	if med.x<112 and
				(btnp(ğŸ…¾ï¸)) and
				nb_bands<20
	then
		nb_bands +=5
		if (nb_bands>20) nb_bands=20
	end
end
